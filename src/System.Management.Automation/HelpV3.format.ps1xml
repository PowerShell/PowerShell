                        <NewLine/>
                        <ExpressionBinding>
                          <ScriptBlock>
                            function GetParam
                            {
                            if(-not $_.Parameters)
                            {
                              return $null
                            }
                            $_.Parameters.Parameter |
                            %{
                            
                            if($_.type)
                            {
                            $param = "[$($_.type.name)] `$$($_.name), "
                            }
                            else
                            {
                            $param = "[object] `$$($_.name), "
                            }
                            
                            $params += $param
                            }

                            $params = $params.Remove($params.Length - 2)
                            return $params
                            }

                            $paramOutput = GetParam

                            "[" + $_.returnValue.type.name + "] " + $_.title + "(" + $($paramOutput) + ")"
