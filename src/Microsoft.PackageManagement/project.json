{
  "name": "Microsoft.PackageManagement",
  "version": "1.0.0-*",

  "buildOptions": {
      "warningsAsErrors": true
  },

  "configurations": {
      "Linux": {
          "buildOptions": {
              "define": [ "UNIX" ]
          }
      }
  },

  "dependencies": {
		  "System.Management.Automation": "1.0.0-*"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.Numerics": "",
        "System.Data": "",
        "System.DirectoryServices": "",
        "System.Security": "",
        "System.Transactions": "",
        "System.Runtime.Serialization": "",
        "System.Management": "",
        "System.Configuration": "",
        "System.Configuration.Install": "",
        "System.Net": "",
        "System.Net.Http": "",
        "System.IO.Compression": "",
        "System.IO.Compression.FileSystem": ""
      },
      "buildOptions": {
        "compile": {
          "exclude": [ "Providers/Inbox/Common/*" ]
        }
      }
    },

    "netstandard1.6": {
      "imports": [ "dnxcore50" ],
      "buildOptions": {
        "define": [ "CORECLR", "PORTABLE" ],
        "debugType": "portable",
      	"compile": {
          "exclude": [ "Providers/Inbox/Common/*" ]
				}
      }
    }
  }
}
