                            <EnumerateCollection/>
                            <CustomControlName>MamlParameterControl</CustomControlName>
                          </ExpressionBinding>
                          <ExpressionBinding>
                            <CustomControlName>CommonWorkflowParametersControl</CustomControlName>
                          </ExpressionBinding>
                          <Text>[</Text>
