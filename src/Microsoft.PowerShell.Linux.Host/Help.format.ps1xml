          <CustomEntry>
            <CustomItem>
              <ExpressionBinding>
                <ScriptBlock>if ($_.required -ne "true") { " [&lt;$_&gt;]" } else { " &lt;$_&gt;" }</ScriptBlock>
              </ExpressionBinding>
            </CustomItem>
          </CustomEntry>
        </CustomEntries>
      </CustomControl>
